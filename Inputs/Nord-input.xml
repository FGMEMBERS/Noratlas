                        <value>0</value>
             </binding>
        </key>

        <key n="110">
            <name>n</name>
            <binding>
                        <command>property-assign</command>
                        <property>sim/model/dummy</property>
                        <value>0</value>
             </binding>
        </key>

    </keyboard>
</PropertyList>