<PropertyList>

 <path>Models/cutoff-nord.ac</path>
 <texture-path>Models/Textures</texture-path>

 	<!--<animation>
		<type>noshadow</type>
		<object-name></object-name>
	</animation>


	<animation include="Instrument-Light4.xml">
		<object-name></object-name>
	</animation>-->

	<model>
		<name>Bipolaire</name>
		<path>Aircraft/Noratlas/Instruments/Bipolaire-Nord.xml</path>
		<offsets>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
			<pitch-deg>0</pitch-deg>
		</offsets>
	</model>



	<animation>
		<type>rotate</type>
		<object-name>Interrupteur</object-name>
		<property>/controls/engines/engine[2]/cutoff</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>0.99</ind>
				<dep>-30</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>30</dep>
			</entry>
		</interpolation>

		<center>
			<x-m>0.0</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
