            <traverse>
                    <setting>
                            <position>0</position>
                            <time>0</time>
                    </setting>
                    <setting>
                            <position>1</position>
                            <time>8</time>
                    </setting>
            </traverse>
            <output>fcs/ar-d-porte-pos-norm</output>
            </kinematic>

            <switch name="Rampe Cmd Norm">
                <default value="0"/>
                <test  logic="AND" value="1">
                    fcs/ar-porte-cmd-norm == 1
                    /controls/gear/brake-parking == 1
