<?xml version="1.0"?>

 <!--Originally From FlightGear Generic Models.
 Modified, testing  some improvements, and looking for a better eye candy, must be FIXED-->


<PropertyList>
 <path>parachuter.ac</path>
 <offsets>
   <pitch-deg>90</pitch-deg>
 </offsets>


 <animation>
  <type>translate</type>
  <object-name>parachute</object-name>
  <property>sim/time/elapsed-sec</property>
  <offset>0</offset>
  <factor>3</factor>
  <max-m>16</max-m>
  <axis>
    <z>1</z>
  </axis>
 </animation>


 <animation>
  <type>scale</type>
  <object-name>parachute</object-name>
  <property>sim/time/elapsed-sec</property>
  <offset>0</offset>
  <x-max>20</x-max>
  <y-max>20</y-max>
  <z-max>1</z-max>
  <x-factor>5</x-factor>
  <y-factor>5</y-factor>
  <z-factor>1</z-factor>
 </animation>


</PropertyList>
