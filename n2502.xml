       <setting>
          <position>  0 </position>
          <time>      0 </time>
       </setting>
       <setting>
          <position> 10 </position>
          <time>      6 </time>
       </setting>
       <setting>
          <position> 20 </position>
          <time>      6 </time>
       </setting>
       <setting>
